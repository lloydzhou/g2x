{
  "name": "antv-g2-react",
  "version": "0.1.2",
  "description": "React component wrapper for antv@g2",
  "repository": {
    "type": "git",
    "url": "https://github.com/lloydzhou/antv-g2-react.git"
  },
  "author": "lloydzhou",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "typescript",
    "tsx",
    "antv",
    "g2"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "dev": "vite",
     "lib": "tsc --jsx react-jsx src/index.ts --esModuleInterop --skipLibCheck --allowSyntheticDefaultImports -t es5 -d --outDir lib",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@antv/g2": "^5.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
